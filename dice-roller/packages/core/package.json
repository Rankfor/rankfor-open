{
  "name": "@rankfor/dice-roller",
  "version": "1.3.0",
  "description": "AI response stability analyzer - measure how consistently LLMs recommend your brand across Gemini, OpenAI, and Grok",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "llm",
    "brand",
    "visibility",
    "stability",
    "gemini",
    "openai",
    "grok",
    "xai",
    "gpt",
    "ai-visibility",
    "brand-monitoring",
    "llm-analysis",
    "response-consistency",
    "shannon-entropy",
    "gini-coefficient",
    "jaccard-similarity",
    "cross-model-analysis",
    "ai-recommendations"
  ],
  "author": {
    "name": "Rankfor.AI",
    "email": "opensource@rankfor.ai",
    "url": "https://rankfor.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rankfor/rankfor-open.git",
    "directory": "dice-roller/packages/core"
  },
  "bugs": {
    "url": "https://github.com/Rankfor/rankfor-open/issues"
  },
  "homepage": "https://github.com/Rankfor/rankfor-open/tree/main/dice-roller/packages/core#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Rankfor"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@google/generative-ai": ">=0.1.0",
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@google/generative-ai": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@google/generative-ai": "^0.21.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "jest": "^29.7.0",
    "jiti": "^2.6.1",
    "openai": "^4.70.0",
    "stopword": "^3.1.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.53.1"
  }
}
