{
  "name": "@rankfor/brand-detector",
  "version": "1.0.1",
  "description": "Detect and analyze brand mentions in LLM/AI responses with 10k+ brand database from Simple Icons and Wikidata",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "data",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "typecheck": "tsc --noEmit",
    "generate-db": "node scripts/fetch-brands.js"
  },
  "keywords": [
    "brand",
    "detection",
    "llm",
    "ai",
    "brand-mentions",
    "competitive-intelligence",
    "ai-visibility",
    "brand-monitoring",
    "simple-icons",
    "wikidata",
    "brand-analysis",
    "llm-response",
    "brand-tracking",
    "sentiment-analysis",
    "gender-bias",
    "recommendation-share"
  ],
  "author": {
    "name": "Rankfor.AI",
    "email": "opensource@rankfor.ai",
    "url": "https://rankfor.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rankfor/rankfor-open.git",
    "directory": "brand-detector/packages/core"
  },
  "bugs": {
    "url": "https://github.com/Rankfor/rankfor-open/issues"
  },
  "homepage": "https://github.com/Rankfor/rankfor-open/tree/main/brand-detector/packages/core#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Rankfor"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3"
  }
}
